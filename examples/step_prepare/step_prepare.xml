<?xml version="1.0" encoding="UTF-8"?>
<jube>
  <benchmark name="step_prepare_example" outpath="bench_run">
    <comment>A step prepare example</comment>
     
    <step name="prepare_step" prepare="execution_step">
      <do>python3 --version || exit 1</do>
    </step>

    <step name="execution_step" tag="execute">
      <do>python3 -c 'import math; print(math.factorial(10))'</do>
    </step>

  </benchmark>
</jube>
