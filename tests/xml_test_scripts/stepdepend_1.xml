<?xml version="1.0" encoding="UTF-8"?>
<jube>
  <benchmark name="stepdepend_test_1" outpath="bench_run">
    <comment>stepdepend_test</comment>
     
    <parameterset name="options">
      <parameter name="option_1">1</parameter>
    </parameterset>

    <step name="step_1">
      <use>options</use>
      <do>echo ${option_1}</do>
    </step>

    <step name="step_2" depend="step_1">
      <use>options</use>
      <do>echo ${option_1}</do>
    </step>

    <step name="step_3" depend="step_1,step_2">
      <use>options</use>
      <do>echo ${option_1}</do>
    </step>

  </benchmark>
</jube>
