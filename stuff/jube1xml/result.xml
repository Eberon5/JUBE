<result>
    <show active="1" colw="30" title="Program version">
    descript 
    </show>
    <show active="1" colw="70" title="Compilation">
    optflags
    </show>
    <show active="1" colw="70" title="Linking">
    ldoptflags
    </show>
    <show active="1" colw="8" title="Input parameter">
    N1, N2, N3, NTST, NSON
    </show>
    <show active="1" colw="15" title="Run parameter">
    ncpus, taskspernode, threadspertask, jobenddate
    </show>
    <show active="1" colw="60" title="Verification">
    vcomment
    </show>
    <show active="1" colw="10" digits="4" title="Resources">
    walltime, itertime_avg, itertime_min, itertime_max , cputtime
    </show>
    <sort>
    ncpus, walltime, cputtime
    </sort>

</result>
